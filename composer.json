{
  "name": "plaisio/obfuscator-core",
  "description": "PhpPlaisio: Core Obfuscator",
  "keywords": [
    "PhpPlaisio",
    "Obfuscate",
    "Obfuscator"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "plaisio/obfuscator": "^1.1",
    "psr/log": "^1.1",
    "setbased/exception": "^2.1",
    "setbased/php-stratum-middle": "^5.2",
    "setbased/php-stratum-mysql": "^5.0",
    "symfony/console": "^5.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
