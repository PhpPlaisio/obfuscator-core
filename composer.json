{
  "name": "plaisio/obfuscator-core",
  "description": "PhpPlaisio: Core Obfuscator",
  "keywords": [
    "PhpPlaisio",
    "Obfuscate",
    "Obfuscator"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "plaisio/console": "^3.0.0",
    "plaisio/obfuscator": "^2.1.0",
    "psr/log": "^3.0.0",
    "setbased/exception": "^2.3.0",
    "setbased/helper-cast": "^3.0.0",
    "setbased/php-stratum-middle": "^5.12.0",
    "setbased/php-stratum-mysql": "^7.0.0",
    "symfony/console": "^6.0|^7.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "setbased/error-handler": "^1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Obfuscator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Obfuscator\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
