{
  "name": "setbased/abc-obfuscator-core",
  "description": "ABC-Framework: Core Obfuscator",
  "keywords": [
    "ABC-Framework",
    "Obfuscate",
    "Obfuscator"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "setbased/abc-obfuscator": "^2.0.0",
    "setbased/exception": "^1.0.0",
    "setbased/php-stratum": "^0.9.54",
    "symfony/console": "^3.0.0 || ^4.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^6.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
