{
  "name": "plaisio/obfuscator-core",
  "description": "PhpPlaisio: Core Obfuscator",
  "keywords": [
    "PhpPlaisio",
    "Obfuscate",
    "Obfuscator"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "plaisio/obfuscator": "^3.0",
    "psr/log": "^1.1.0",
    "setbased/exception": "^2.0",
    "setbased/php-stratum-middle": "^5.0.1",
    "setbased/php-stratum-mysql": "^4.1.0",
    "symfony/console": "^4.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
